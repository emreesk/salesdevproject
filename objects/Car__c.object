    <fields>
        <fullName>Vehicle_Tax__c</fullName>
        <externalId>false</externalId>
        <formula>IF(AND( Vehicle_Length_Meters__c  &gt; 0,  Vehicle_Length_Meters__c &lt;= 3), Vehicle_Cost__c * 0.1,
IF(AND( Vehicle_Length_Meters__c &gt; 3,  Vehicle_Length_Meters__c &lt;= 6), Vehicle_Cost__c * 0.2,
IF(AND( Vehicle_Length_Meters__c &gt; 6,  Vehicle_Length_Meters__c &lt;= 10), Vehicle_Cost__c * 0.3,
0)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
